{
  "name": "nina-web-control-panel",
  "version": "1.1.0",
  "description": "A dashboard for monitoring and controlling astro gear in a remote observatory using NINA.",
  "main": "src/client/app/index.tsx",
  "scripts": {
    "start": "node start-dev.js",
    "start:windows": "start-windows.bat",
    "start:dev": "node start-dev.js",
    "start:logged": "node start-dev-logged.js",
    "start:debug": "DEBUG=true node start-dev.js",
    "start:debug-websocket": "DEBUG_WEBSOCKET=verbose node start-dev.js",
    "start:debug-all": "DEBUG=all DEBUG_WEBSOCKET=verbose node start-dev.js",
    "start:enhanced": "node start-enhanced.js --enhanced",
    "start:enhanced-debug": "node start-enhanced.js --enhanced --debug",
    "start:legacy": "node start-enhanced.js",
    "start:prod": "node scripts/deployment/process-manager.js start",
    "start:stable": "node scripts/monitoring/backend-monitor.js",
    "build": "cd src/client && npm run build",
    "test": "cd src/client && npm test",
    "server": "node src/server/config-server.js",
    "server:enhanced": "USE_ENHANCED_SESSION_MANAGER=true node src/server/config-server-enhanced.js",
    "client": "cd src/client && npm start",
    "validate": "node scripts/development/config-validator.js",
    "deploy": "node scripts/deployment/deploy-production-fixed.js",
    "deploy:original": "node scripts/deployment/deploy-production.js",
    "health": "node scripts/monitoring/health-monitor.js",
    "monitor": "node scripts/monitoring/backend-monitor.js",
    "fix-backend": "node scripts/development/backend-stability-fix.js",
    "reset-db": "pwsh -File scripts/database/reset-dashboard-database.ps1",
    "generate-icons": "node scripts/development/generate-icons.js",
    "install-imagemagick": "pwsh -File scripts/development/install-imagemagick.ps1",
    "logs": "pwsh -Command \"Get-Content logs/*.log -Tail 50 -Wait\"",
    "logs:backend": "pwsh -Command \"Get-Content logs/backend-*.log -Tail 50 -Wait\"",
    "logs:frontend": "pwsh -Command \"Get-Content logs/frontend-*.log -Tail 50 -Wait\"",
    "logs:clean": "pwsh -Command \"Remove-Item logs/*.log -Force\"",
    "apply-server-fix": "copy src\\server\\config-server.enhanced.js src\\server\\config-server.js",
    "stop": "node scripts/deployment/process-manager.js stop",
    "restart": "node scripts/deployment/process-manager.js restart",
    "status": "node scripts/deployment/process-manager.js status"
  },
  "dependencies": {
    "@fontsource/space-grotesk": "^5.2.8",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react-grid-layout": "^1.3.5",
    "axios": "^1.11.0",
    "better-sqlite3": "^11.8.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "moment-timezone": "^0.6.0",
    "os-utils": "^0.0.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.2",
    "react-scripts": "^5.0.1",
    "socket.io-client": "^4.0.0",
    "systeminformation": "^5.27.8",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "typescript": "^4.1.2"
  },
  "homepage": ".",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
